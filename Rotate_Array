#include <bits/stdc++.h>
using namespace std;
int main()
 {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int n,i;
	    cin>>n;
	    int k;
	    cin>>k;
	    vector <int> v ;
	    for(i=0;i<n;i++)
	    {
	        int temp;
	        cin>>temp;
	        v.push_back(temp);
	    }
	    
	  
	    for(i=k;i<n;i++)
	    {
	          cout<<v[i]<<" ";
	    }
	    
	    for(i=0;i<k;i++)
	    {
	          cout<<v[i]<<" ";
	    }
	    cout<<endl;
	}
	return 0;
}
